<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>GCode_Editor</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>G-Code EditorV2.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="G-Code EditorV2.ico" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="CustomProgressBar.cs">
      <Generator></Generator>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="CustomProgressBar.resx">
      <Generator></Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>